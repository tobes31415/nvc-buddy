{
  "name": "nvc_buddy",
  "version": "1.0.0",
  "description": "A tool to help you walk through the NVC process",
  "scripts": {
    "test": "test",
    "tsc": "tsc",
    "format": "prettier --write src/",
    "start": "parcel src/index.html",
    "build": "run-s eslint pb:clean pb:compile",
    "build:compile": "parcel build src/index.html",
    "build:clean": "del-cli dist/**",
    "eslint": "eslint \"src/**.ts\" --fix",
    "firebase": "firebase"
  },
  "keywords": [
    "nvc",
    "non",
    "violent",
    "communication",
    "needs",
    "feelings",
    "thoughts",
    "judgements",
    "chatbot"
  ],
  "author": "Jake Tober",
  "license": "MIT",
  "dependencies": {
    "firebase": "^9.6.9",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "cpy-cli": "^4.1.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.11.0",
    "firebase-tools": "^10.4.0",
    "parcel": "^2.3.2",
    "prettier": "2.6.0"
  }
}
